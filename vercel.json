{
    "functions": {
    "api/*": { "maxDuration": 60 }
  },
  "build": {
    "maxDuration": 600
  },
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  
